        "import cv2\n",
        "import pytesseract\n",
        "from PIL import Image\n",
        "\n",
        "# Load the saved image\n",
        "image = cv2.imread(image_path, cv2.IMREAD_GRAYSCALE)\n",
        "\n",
        "# Apply thresholding to enhance contrast\n",
        "image = cv2.threshold(image, 150, 255, cv2.THRESH_BINARY_INV + cv2.THRESH_OTSU)[1]\n",
        "\n",
        "# Save and reload as PIL Image for OCR\n",
        "cv2.imwrite(\"processed_image.png\", image)\n",
        "image_pil = Image.open(\"processed_image.png\")\n",
        "\n",
        "# Run OCR\n",
        "text = pytesseract.image_to_string(image_pil)\n",
        "\n",
        "# Print extracted text\n",
        "print(\"Extracted Text:\\n\", text)\n"
