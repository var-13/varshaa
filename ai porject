        "from pdf2image import convert_from_path\n",
        "\n",
        "try:\n",
        "    pdf_path = \"your_uploaded_file.pdf\"  # Replace with actual uploaded file name\n",
        "    images = convert_from_path(pdf_path)\n",
        "\n",
        "    # Check if images were generated\n",
        "    if not images:\n",
        "        raise ValueError(\"No images extracted from the PDF. The PDF might be encrypted or empty.\")\n",
        "\n",
        "    # Save each page as an image\n",
        "    image_paths = []\n",
        "    for i, image in enumerate(images):\n",
        "        img_path = f\"page_{i+1}.png\"\n",
        "        image.save(img_path, \"PNG\")\n",
        "        image_paths.append(img_path)\n",
        "\n",
        "    print(f\"Successfully saved {len(image_paths)} pages as images:\", image_paths)\n",
        "\n",
        "except Exception as e:\n",
        "    print(f\"Error processing PDF: {e}\")\n"
